public class hw2{

    int[] array = new int[3];

    static void main(String[] args){
        for(int i = 0; i < 3; i++){
            int num = (int)Math.floor(Math.random() * 9) + 2;
            num += array[i];
        }

        for(int i = 0; i < 3; i++) {
            System.out.println(array[i]);
        }
    }
}