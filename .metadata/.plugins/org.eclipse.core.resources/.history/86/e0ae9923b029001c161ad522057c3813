public class CardDeck{

    int[] array;

    for(int i = 0; i < array.length; i++){
        int num = (int)Math.floor(Math.random() * 9) + 2;
        num += array[i];
    }

    public static void main(String[] args){
        for(int i = 0; i < 3; i++) {
            System.out.println(array[i]);
        }
    }
}